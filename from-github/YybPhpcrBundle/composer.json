{
    "name": "yyb/phpcr-bundle",
    "description": "Primitive graphing infrastructure for phpcr-odm",
    "type": "symfony-bundle",
    "keywords": ["phpcr", "odm", "persistence", "graph", "tree", "visitor"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jolly Blume",
            "email": "jollyblume@gmail.com",
            "role": "Developer"
        }
    ],
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "YoYogaBear\\Bundle\\PhpcrBundle\\": ""
        }
    },
    "require": {
        "php": "~7.1",
        "symfony/http-kernel": "^3.2.4",
        "doctrine/collections": "~1.4.0",
        "jackalope/jackalope": "^1.2",
        "jackalope/jackalope-doctrine-dbal": "^1.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/phpcr-odm": "^1.4",
        "doctrine/phpcr-bundle": "^1.3"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "^6.0@dev",
        "phpunit/php-code-coverage": "^5.0@dev",
        "friendsofphp/php-cs-fixer": "^2.1",
        "matthiasnoback/symfony-dependency-injection-test": "dev-master",
        "sensio/distribution-bundle": "^5.0"
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "extra": {
        "symfony-app-dir": "Tests/Resources/app",
        "symfony-bin-dir": "Tests/Resources/bin",
        "symfony-var-dir": "Tests/Resources/var",
        "symfony-web-dir": "Tests/Resources/web",
        "branch-alias": {
            "dev-master": "v1.0.x-dev"
        }
    }
}
